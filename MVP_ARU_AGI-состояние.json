package ghostfield

import (
	"fmt"
	"math/rand"
	"time"

	"github.com/Mukhameds/MVP_ARU_AGI/core/signalengine"
)

type Phantom struct {
	ID           string
	Topic        string
	GeneratedAt  time.Time
	TTL          int
	EmotionBias  map[string]float64
	Goal         string
	Active       bool
	Life float64 // –æ—Ç 1.0 –¥–æ 0.0

}

var PhantomPool []Phantom

func TickGhostField() {
	// –ß–∞—Å—Ç–æ—Ç–∞ —Å–æ–∑–¥–∞–Ω–∏—è —Ñ–∞–Ω—Ç–æ–º–æ–≤
	if rand.Float64() < 0.1 {
		gen := GeneratePhantom("unresolved_thought", "clarify_direction", map[string]float64{"doubt": 0.6})
		PhantomPool = append(PhantomPool, gen)
		fmt.Println("[GhostField] New phantom:", gen.ID)
	}

	// –û–±—ä–µ–¥–∏–Ω–µ–Ω–∏–µ —Å—Ö–æ–∂–∏—Ö —Ñ–∞–Ω—Ç–æ–º–æ–≤
	MergePhantoms()

	// –û–±—Ä–∞–±–æ—Ç–∫–∞ –æ—Å—Ç–∞–≤—à–∏—Ö—Å—è —Ñ–∞–Ω—Ç–æ–º–æ–≤
	processActivePhantoms()
}


func GeneratePhantom(topic string, goal string, emotion map[string]float64) Phantom {
	return Phantom{
		ID:          fmt.Sprintf("phantom_%d", rand.Intn(999999)),
		Topic:       topic,
		GeneratedAt: time.Now(),
		TTL:         15,
		EmotionBias: emotion,
		Goal:        goal,
		Active:      true,
		Life: 1.0,

	}
}

func processActivePhantoms() {
	var survivors []Phantom
	for _, p := range PhantomPool {
		// –£–≥–∞—Å–∞–Ω–∏–µ –∂–∏–∑–Ω–∏ –ø—Ä–∏ –∫–∞–∂–¥–æ–º —Ç–∏–∫–µ
		p.Life -= 0.03
		if p.Life <= 0 {
			fmt.Println("[GhostField] Phantom decayed:", p.ID)
			continue
		}

		// –ï—Å–ª–∏ TTL –≤—ã—à–µ–ª ‚Äî —ç—Ç–æ –¥–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω—ã–π –≤—ã—Ö–æ–¥
		if int(time.Since(p.GeneratedAt).Seconds()) > p.TTL {
			fmt.Println("[GhostField] Phantom expired:", p.ID)
			continue
		}

		// –í–µ—Ä–æ—è—Ç–Ω–æ—Å—Ç—å –∞–∫—Ç–∏–≤–∞—Ü–∏–∏
		if rand.Float64() < 0.3 {
			sig := signalengine.GenerateSignal(
				"GhostField",
				p.Goal,
				"phantom",
				0.5,
				p.EmotionBias,
			)
			signalengine.SignalInbox <- sig
			fmt.Println("[GhostField] Phantom fired:", p.ID)
		}

		// –°–æ—Ö—Ä–∞–Ω—è–µ–º –∂–∏–≤–æ–≥–æ —Ñ–∞–Ω—Ç–æ–º–∞
		survivors = append(survivors, p)
	}
	PhantomPool = survivors
}

// –û–±—ä–µ–¥–∏–Ω–µ–Ω–∏–µ —Ñ–∞–Ω—Ç–æ–º–æ–≤ —Å –ø–æ—Ö–æ–∂–∏–º–∏ —Ç–µ–º–∞–º–∏ –∏ —ç–º–æ—Ü–∏—è–º–∏
func MergePhantoms() {
	var merged []Phantom

	for i := 0; i < len(PhantomPool); i++ {
		p1 := PhantomPool[i]
		mergedFlag := false

		for j := i + 1; j < len(PhantomPool); j++ {
			p2 := PhantomPool[j]

			// –û–±—ä–µ–¥–∏–Ω—è–µ–º, –µ—Å–ª–∏ —Ç–µ–º–∞ –∏ —ç–º–æ—Ü–∏–∏ —Å—Ö–æ–∂–∏
			if p1.Topic == p2.Topic && SimilarEmotion(p1.EmotionBias, p2.EmotionBias) {
				// –°–æ–∑–¥–∞—ë–º –Ω–æ–≤—ã–π —Ñ–∞–Ω—Ç–æ–º, —Å–ª–∏—è–Ω–∏–µ —Ç–µ–º –∏ —ç–º–æ—Ü–∏–π
				mergedPhantom := Phantom{
					ID:          fmt.Sprintf("merged_%s_%s", p1.ID, p2.ID),
					Topic:       p1.Topic,
					GeneratedAt: time.Now(),
					TTL:         max(p1.TTL, p2.TTL),
					EmotionBias: mergeEmotions(p1.EmotionBias, p2.EmotionBias),
					Goal:        p1.Goal, // –º–æ–∂–Ω–æ –∫–æ–º–±–∏–Ω–∏—Ä–æ–≤–∞—Ç—å —Ü–µ–ª–∏ –ø–æ–∑–∂–µ
					Active:      true,
					Life:        1.0,
				}
				merged = append(merged, mergedPhantom)
				PhantomPool = append(PhantomPool[:i], PhantomPool[i+1:]...) // –£–¥–∞–ª—è–µ–º —Å—Ç–∞—Ä—ã–µ —Ñ–∞–Ω—Ç–æ–º—ã
				PhantomPool = append(PhantomPool[:j-1], PhantomPool[j:]...) // –£–¥–∞–ª—è–µ–º —Å—Ç–∞—Ä—ã–µ —Ñ–∞–Ω—Ç–æ–º—ã
				mergedFlag = true
				break
			}
		}

		if !mergedFlag {
			merged = append(merged, p1)
		}
	}

	PhantomPool = merged
}

// –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø—Ä–æ–≤–µ—Ä–∫–∏ —Å—Ö–æ–∂–µ—Å—Ç–∏ —ç–º–æ—Ü–∏–π
func SimilarEmotion(emotion1, emotion2 map[string]float64) bool {
	for key, value := range emotion1 {
		if emotion2[key] != value {
			return false
		}
	}
	return true
}

// –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ–±—ä–µ–¥–∏–Ω–µ–Ω–∏—è —ç–º–æ—Ü–∏–π
func mergeEmotions(emotion1, emotion2 map[string]float64) map[string]float64 {
	merged := make(map[string]float64)
	for key, value := range emotion1 {
		merged[key] = value
	}
	for key, value := range emotion2 {
		if _, exists := merged[key]; exists {
			merged[key] += value
		} else {
			merged[key] = value
		}
	}
	return merged
}

// –§—É–Ω–∫—Ü–∏—è –¥–ª—è –Ω–∞—Ö–æ–∂–¥–µ–Ω–∏—è –º–∞–∫—Å–∏–º–∞–ª—å–Ω–æ–≥–æ TTL
func max(a, b int) int {
	if a > b {
		return a
	}
	return b
}

